config {
  type: "table",
  bigquery: { partitionBy: "DATE(LOAD_DTS)", clusterBy: ["HK_AirportCode"] },
  schema: "raw_vault",
  tags: ["satellite"]
}

SELECT
  TO_HEX(MD5(AirportCode)) AS HK_AirportCode,
  AirportName,
  CURRENT_TIMESTAMP() AS LOAD_DTS,
  'AI_AIRPORT_DETAILS' AS REC_SRC
FROM ${ref("AI_AIRPORT_DETAILS")}
WHERE AirportCode IS NOT NULL
GROUP BY AirportCode, AirportName