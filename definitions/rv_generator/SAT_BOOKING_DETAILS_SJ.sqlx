config {
  type: "table",
  schema: "raw_vault",
  tags: ["satellite"]
}

SELECT
  MD5(BookingID) AS HK_BookingID,
  Status,
  BookingDate,
  SeatNumber,
  SeatClass,
  CreatedDateTime,
  LastupdateDateTime,
  CURRENT_TIMESTAMP() AS LOAD_DTS,
  'SJ_BOOKING_DETAILS' AS REC_SRC
FROM ${ref("SJ_BOOKING_DETAILS")}
WHERE BookingID IS NOT NULL
GROUP BY BookingID, Status, BookingDate, SeatNumber, SeatClass, CreatedDateTime, LastupdateDateTime