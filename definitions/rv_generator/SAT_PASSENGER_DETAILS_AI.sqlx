config {
  type: "table",
  schema: "raw_vault",
  tags: ["satellite"]
}

SELECT
  MD5(PassengerID) AS HK_PassengerID,
  FirstName,
  LastName,
  DOB,
  Email,
  CreatedDateTime,
  LastupdateDateTime,
  CURRENT_TIMESTAMP() AS LOAD_DTS,
  '' AS REC_SRC
FROM ${ref("")}
WHERE PassengerID IS NOT NULL
GROUP BY PassengerID, FirstName, LastName, DOB, Email, CreatedDateTime, LastupdateDateTime