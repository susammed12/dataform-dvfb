config {
  type: "table",
  schema: "raw_vault",
  tags: ["link"]
}

SELECT
  MD5(BOOKINGID) AS HK_BOOKINGID,
  MD5(PASSENGERID) AS HK_PASSENGERID,
  CURRENT_TIMESTAMP() AS LOAD_DTS,
  'AI_BOOKING_PAYMENT_DETAILS' AS REC_SRC
FROM ${ref("BOOKING_PAYMENT_DETAILS")}
WHERE BOOKINGID IS NOT NULL AND PASSENGERID IS NOT NULL
GROUP BY BOOKINGID,PASSENGERID